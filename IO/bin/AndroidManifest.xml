<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="whjz.io.activity"
    android:versionCode="1"
    android:versionName="1.0"   >


    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="15" />
    
    
     <!--操作SD卡-->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/>
    
    
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    
    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
         <activity android:name=".MainActivity"              
                 android:label="@string/app_name" >    
             <intent-filter>                   
                  <action android:name="android.intent.action.MAIN" />
                  <category android:name="android.intent.category.LAUNCHER" />     
            </intent-filter>     
         </activity>
         <activity android:name=".UartActivity"  android:label="@string/MAIN_UART"></activity>
         <activity android:name=".ADActivity" android:label="@string/MAIN_AD"></activity>
         <activity android:name=".BluActivity"  android:label="@string/MAIN_BLU"></activity>
         <activity android:name=".BuzzerActivity"  android:label="@string/MAIN_BUZ"></activity>
         <activity android:name=".GpioActivity"  android:label="@string/MAIN_GPIO"></activity>
         <activity android:name=".GprsActivity"  android:label="@string/MAIN_GPRS"></activity>
         <activity android:name=".PWMActivity"  android:label="@string/MAIN_PWM"></activity>
         <activity android:name=".Rs485Activity"  android:label="@string/MAIN_RS485"></activity>
         <activity android:name=".TempSensorActivity"  android:label="@string/MAIN_TEMP"></activity>
         <activity android:name=".ZigbeeActivity"   android:label="@string/MAIN_ZIG"></activity>
         <activity android:name=".TestActivity"   android:label="test"></activity>
    </application>
</manifest>